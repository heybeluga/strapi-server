{
  "kind": "collectionType",
  "collectionName": "article_entries",
  "info": {
    "singularName": "article-entry",
    "pluralName": "article-entries",
    "displayName": "ArticleEntry",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ArticleTitle": {
      "type": "string"
    },
    "titleImage": {
      "type": "component",
      "repeatable": true,
      "component": "general.image"
    },
    "articleAuthor": {
      "type": "string"
    },
    "articleText": {
      "type": "component",
      "repeatable": true,
      "component": "articles.paragraph"
    },
    "articleSlug": {
      "type": "string"
    },
    "migratedPublishedDate": {
      "type": "date"
    },
    "featuredArticle": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "articleDescription": {
      "type": "string",
      "maxLength": 255
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "ArticleSummary": {
      "type": "component",
      "repeatable": true,
      "component": "general.bullet"
    }
  },
  "indexes": [
    {
      "name": "index_migrated_published_date",
      "columns": ["migrated_published_date"]
    }
  ]
}
