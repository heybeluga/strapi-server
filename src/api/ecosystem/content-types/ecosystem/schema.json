      "type": "text",
      "required": true
    },
    "activitiesDescription": {
      "type": "text",
      "required": true
    },
    "subBrandsDescription": {
      "type": "text",
      "required": true
    },
    "recDescription": {
      "type": "text",
      "required": true
    },
    "ecosystem_activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ecosystem-activity.ecosystem-activity",
      "mappedBy": "ecosystem"
    },
    "twitterUrl": {
      "type": "string",
      "required": true
    },
    "telegramUrl": {
      "type": "string",
      "required": true
    },
    "discordUrl": {
      "type": "string",
      "required": true
