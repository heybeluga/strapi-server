      "allowedTypes": [
        "images"
      ]
    },
    "ecosystemTitle": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "ecosystemDescription": {
      "type": "text",
      "required": true,
      "unique": false
    },
    "ecosystemSlug": {
      "type": "string",
      "required": true,
