    "tokenLaunchDate": {
      "type": "date"
    },
    "sector": {
      "type": "enumeration",
      "enum": [
        "AI",
        "Depin",
        "Infrastructure",
        "L1",
        "Defi"
      ]
    },
    "ecosystemPage": {
      "type": "string"
    },
